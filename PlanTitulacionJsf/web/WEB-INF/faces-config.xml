<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>irHomeEstudiante</from-outcome>
            <to-view-id>/planTitulacion/estudiante/homeEstudiante.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>irHomeDocente</from-outcome>
            <to-view-id>/planTitulacion/docente/homeDocente.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>irHomeConsejo</from-outcome>
            <to-view-id>/planTitulacion/consejo/homeConsejo.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>irPrincipal</from-outcome>
            <to-view-id>/planTitulacion/generalidades/principal.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        <navigation-case>
            <from-outcome>irInicio</from-outcome>
            <to-view-id>/planTitulacion/generalidades/inicio.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
</faces-config>
